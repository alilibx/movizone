    "chalk": "5",
    "cli-table3": "^0.6.5",
    "figlet": "^1.10.0",
    "gradient-string": "^3.0.0",
    "inquirer": "12",
    "ora": "8",
    "webtorrent": "^2.8.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/figlet": "^1.7.0",
    "typescript": "^5"
  },
  "trustedDependencies": [
